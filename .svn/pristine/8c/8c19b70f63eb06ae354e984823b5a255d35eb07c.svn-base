package com.wx.doctor.api;

/**
 * A interface that executes submitted tasks.
 * 
 */
public interface IEngine {

	/**
	 * Submits the task
	 * 
	 * @param task
	 */
	public void submit(ITask task);

	/**
	 * Try to cancel all submitted task and shutdown the task engine.
	 */
	public void shutdown();
}
